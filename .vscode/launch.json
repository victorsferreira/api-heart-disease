    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run Serverless Offline",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "npm",
            "runtimeArgs": [
                "run",
                "debug-vscode"
            ],
            "outFiles": [
                "${workspaceFolder}/dist/src/index.js"
            ],
            "port": 9229,
            "sourceMaps": true,
            "timeout": 25000
        }
    ]
}