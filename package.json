  "scripts": {
    "test": "yarn lint && yarn test-unit && yarn test-integration",
    "test-unit": "mocha -r ts-node/register ./test/unit/**/*.spec.ts",
    "test-integration": "sls offline start --exec \"mocha -r ts-node/register ./test/integration/**/*.spec.ts\"",
    "start": "npm run build && sls offline",
    "build": "rm -rf dist/ && tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "deploy:stg": "npm run build && sls deploy -s stg",
    "deploy:prd": "npm run build && sls deploy -s prd",
    "debug-vscode": "npm run build && export SLS_DEBUG=* && SLS_DEBUG=* node --inspect ./node_modules/.bin/sls offline"
  },
